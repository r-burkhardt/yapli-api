{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-invalid-this": "off",
        "comma-dangle": "off",
        "valid-jsdoc": "off",
        "new-cap": "off",
        "space-before-function-paren": "off",
        "arrow-parens": "off"
    }
}
