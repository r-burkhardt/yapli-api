{
  "name": "express-api",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "npm run build && concurrently --kill-others \"tsc -w\" \"nodemon dist/index.js -e ts\"",
    "debug": "npm run build && concurrently --kill-others \"tsc -w\" \"nodemon --inspect dist/index.js -e ts\"",
    "test:watch": "mocha -r ts-node/register -w ./spec/**/*.spec.ts --watch-extensions ts",
    "test": "mocha -r ts-node/register -w ./spec/**/*.spec.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "build:docker": "docker build -t rjmacarthy/express-typescript-starter .",
    "docker:compose": "docker-compose up",
    "format": "prettier --write 'src/**/*ts'",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "lint:test": "tslint -c tslint.json 'spec/**/*.ts'",
    "watch-ts": "tsc -w",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript, Node\" -c \"yello.bold, cyan.bold\" \"npm run watch-ts\" \"npm run watch-node\""
  },
  "keywords": [],
  "author": "Roderick Nagashima",
  "license": "ISC",
  "dependencies": {
    "@types/cors": "^2.8.6",
    "@types/mongodb": "^3.5.25",
    "@types/mysql": "^2.15.9",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "http-status-codes": "^1.4.0",
    "mongodb": "^3.5.9",
    "morgan": "^1.9.1",
    "mysql": "^2.18.1",
    "path": "^0.12.7",
    "proper-lockfile": "^4.1.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.2",
    "@types/morgan": "^1.7.37",
    "@types/node": "^13.7.4",
    "@types/proper-lockfile": "^4.1.1",
    "concurrently": "^5.1.0",
    "nodemon": "^2.0.2",
    "ts-node": "^8.6.2",
    "tslint": "^6.0.0",
    "tslint-clean-code": "^0.2.10",
    "typescript": "^3.8.2"
  }
}
